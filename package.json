{
  "name": "bitbucket-clone-all-repos",
  "version": "0.1.0",
  "description": "An app that will clone all your bitbucket repos locally (git)",
  "main": "app.js",
  "scripts": {
    "start": "NODE_ENV=development node app.js",
    "play": "NODE_ENV=development node play.js",
    "test": "NODE_ENV=test nyc mocha spec/**/*.spec.js",
    "report": "NODE_ENV=test nyc report"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vanuwa/bitbucket-clone-all-repos.git"
  },
  "keywords": [
    "app",
    "scipt",
    "bitbucket",
    "clone",
    "repositories"
  ],
  "author": "Va Nuwa",
  "license": "ISC",
  "homepage": "https://github.com/vanuwa/bitbucket-clone-all-repos",
  "engines": {
    "node": "8.9.3"
  },
  "devDependencies": {
    "chai": "^4.1.1",
    "eslint": "^4.13.0",
    "mocha": "^4.0.1",
    "nyc": "^11.2.1"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "config": "^1.26.1",
    "log4js": "^2.3.4"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "lib/**/*.js",
      "spec/**/*.spec.js"
    ]
  }
}
